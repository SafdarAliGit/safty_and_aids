 "html": "{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n\n<div style=\"margin-right:12px; margin-left:12px;\">\n\n<h2 style=\"text-align:center; border: 1.5px solid black; padding:5px; outline: 3px solid black; outline-offset: 2px; font-weight:bold;\">\n  {{ doc.select_print_heading or \"\u0625\u0634\u0639\u0627\u0631 \u0645\u0628\u0627\u0634\u0631\u0629 \u0627\u0644\u0639\u0645\u0644\" }}\n</h2>\n\n<div style=\"display: flex; width: 100%; border: 1.5px solid black; box-sizing: border-box;\">\n    <!-- Left box (40%) -->\n    <div style=\"width: 40%; border-right: none; box-sizing: border-box;\">\n        <div style=\"display: flex; flex-wrap: wrap; width: 100%; box-sizing: border-box;\">\n            <!-- Row 1, Column 1 -->\n            <div style=\"width: 50%; padding: 5px; border-bottom: 1.5px solid black; border-right: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                {{doc.name}}\n            </div>\n            <!-- Row 1, Column 2 -->\n            <div style=\"width: 50%; padding: 5px; border-bottom: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                \u0631\u0642\u0645 \u0627\u0644\u0646\u0645\u0648\u0630\u062c\n            </div>\n            <!-- Row 2, Column 1 -->\n            <div style=\"width: 50%; padding: 5px; border-right: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                {{ frappe.utils.formatdate(frappe.utils.nowdate()) }}\n            </div>\n            <!-- Row 2, Column 2 -->\n            <div style=\"width: 50%; padding: 5px; box-sizing: border-box; font-weight:bold;\">\n                \u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0625\u0635\u062f\u0627\u0631\n            </div>\n        </div>\n    </div>\n    \n    <!-- Right box (60%) -->\n    <div style=\"width: 60%; padding: 5px; border-left: 1.5px solid black; box-sizing: border-box; font-weight:bold; text-align:center;\">\n        \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0