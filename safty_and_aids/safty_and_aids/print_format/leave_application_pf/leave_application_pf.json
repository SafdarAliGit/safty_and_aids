 "html": "<!--{% if letter_head %}-->\n<!--{{ letter_head }} -->\n<!--{% endif %}-->\n\n<div id=\"header-html\" class=\"hidden-pdf\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\", \"PO LH\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  {% else %}\n\t<p>Letter head {{ doc.letter_head }} not found. Please define the letter head under print settings.</p>\n  {% endif %}\n</div>\n\n<div style=\"margin-right:12px; margin-left:12px;\">\n\n<h2 style=\"text-align:center; border: 1.5px solid black; padding:2px; outline: 3px solid black; outline-offset: 2px; font-weight:bold;\">\n  {{ doc.select_print_heading or \" \u0637\u0644\u0628 \u0625\u062c\u0627\u0632\u0629\" }}\n</h2>\n\n<div style=\"display: flex; width: 100%; border: 1.5px solid black; box-sizing: border-box;\">\n    <!-- Left box (40%) -->\n    <div style=\"width: 40%; border-right: none; box-sizing: border-box;\">\n        <div style=\"display: flex; flex-wrap: wrap; width: 100%; box-sizing: border-box;\">\n            <!-- Row 1, Column 1 -->\n            <div style=\"width: 50%; padding: 2px; border-bottom: 1.5px solid black; border-right: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                {{doc.name}}\n            </div>\n            <!-- Row 1, Column 2 -->\n            <div style=\"width: 50%; padding: 2px; border-bottom: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                \u0631\u0642\u0645 \u0627\u0644\u0646\u0645\u0648\u0630\u062c\n            </div>\n            <!-- Row 2, Column 1 -->\n            <div style=\"width: 50%; padding: 2px; border-right: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                {{ frappe.utils.formatdate(frappe.utils.nowdate()) }}\n            </div>\n            <!-- Row 2, Column 2 -->\n            <div style=\"width: 50%; padding: 2px; box-sizing: border-box; font-weight:bold;\">\n                \u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0625\u0635\u062f\u0627\u0631\n            </div>\n        </div>\n    </div>\n    \n    <!-- Right box (60%) -->\n    <div style=\"width: 60%; padding: 2px; border-left: 1.5px solid black; box-sizing: border-box; font-weight:bold; text-align:center;\">\n        \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0644\u0628\u0634\u0631\u064a\u0629 / \u0646\u0645\u0627\u0630\u062c \u0627\u0644\u0639\u0645\u0644<br>HUMAN RESOURCES MANUAL\n    </div>\n</div>\n\n<div style=\"margin-top:5px; width: 100%;  box-sizing: border-box;\">\n    \n    \n<div style=\"display: flex; width: 100%; border:1.5px solid black;\">\n    <div style=\"border-right: 1.5px solid black;  width: 3%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; text-align: center;border-bottom: 1.5px solid black;padding: 2px;\">1</div>\n        <div style=\"height: auto;  writing-mode: vertical-rl; \n            -webkit-transform: rotate(360deg); text-align: center; display: flex; \n            align-items: center; justify-content: center;padding: 2px;\">\n            Emp. Data\n        </div>\n    </div>\n    <div style=\"border-right: 1.5px solid black; width: 94%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; display: flex; align-items: center; justify-content: center;padding:2px;border-bottom:1.5px solid black;\">\n           \n           \n    <div style=\"display: flex; flex-direction: column; width: 100%; \">\n    <!-- First Row: 3 Columns -->\n    <div style=\"display: flex; width: 100%;font-size:0.8em;padding-top:2px;\">\n        <div style=\"flex: 1;  text-align: left;\">Nationality : {{doc.nationality}} : \u0627\u0644\u062c\u0646\u0633\u064a\u0629   </div>\n        <div style=\"flex: 1;  text-align: center;\">No : {{doc.employee_no}} : \u0627\u0644\u0631\u0642\u0645  </div>\n        <div style=\"flex: 1;  text-align: right;\">Name : {{doc.employee_name}} : \u0627\u0644\u0627\u0633\u0645 </div>\n    </div>\n    <!-- Second Row: 2 Columns -->\n    <div style=\"display: flex; width: 100%;font-size:0.8em;padding-top:2px;\">\n        <div style=\"flex: 1;text-align: left;\">Started Work on : {{doc.get_formatted('started_work_on')}} : \u062a\u0627\u0631\u064a\u062e \u0628\u062f\u0627\u064a\u0629 \u0627\u0644\u0639\u0645\u0644 </div>\n        <div style=\"flex: 1; text-align: right;\">job title : {{doc.job_title}} : \u0645\u0633\u0645\u0649 \u0627\u0644\u0648\u0638\u064a\u0641\u0629 </div>\n    </div>\n    \n    <div style=\"display: flex; width: 100%;font-size:0.8em;padding-top:2px;\">\n        <div style=\"flex: 1;  text-align: left;\">Section: {{doc.section}} : \u0627\u0644\u0642\u0633\u0645    </div>\n        <div style=\"flex: 1;  text-align: center;\"> Dept. : {{doc.department}} : \u0627\u0644\u0625\u062f\u0627\u0631\u0629   </div>\n        <div style=\"flex: 1;  text-align: right;\">Contract Period : {{doc.contract_period}} : \u0645\u062f\u0629 \u0627\u0644\u0639\u0642\u062f  </div>\n    </div>\n    \n      <div style=\"display: flex; width: 100%;font-size:0.8em;padding-top:2px;\">\n        <div style=\"flex: 1;  text-align: left;\">From date: {{doc.section}} : \u0625\u0644\u0649     </div>\n        <div style=\"flex: 1;  text-align: center;\"> To  : {{doc.department}} :  \u0645\u0646 \u062a\u0627\u0631\u064a\u062e    </div>\n        <div style=\"flex: 1;  text-align: right;\">Vacation\\days : {{doc.contract_period}} : \u0645\u062f\u0629 \u0627\u0644\u0625\u062c\u0627\u0632\u0629    </div>\n    </div>\n    \n    \n    <!-- Third Row: 2 Columns -->\n    <div style=\"display: flex; width: 100%;font-size:0.8em;\">\n        <div style=\"flex: 1; padding: 2px; text-align: center;\">Nationality : {{doc.nationality}} :\u0627\u0644\u062c\u0646\u0633\u064a\u0629</div>\n        <div style=\"flex: 1; padding: 2px; text-align: center;\">Starting work at : {{doc.get_formatted('starting_work_at')}} :\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0645\u0628\u0627\u0634\u0631\u0629</div>\n    </div>\n\n\n</div>\n</div>\n        \n        \n<div style=\"display: flex; flex-direction: row; width: 100%;\">\n    <!-- First Row: Horizontally Aligned Columns -->\n    <div style=\"display: flex; width: 100%; font-size: 0.8em; padding: 2px;\">\n        \n        <!-- Others Column -->\n        <div style=\"flex: 1; text-align: left;\">\n            Others:\n            {% if doc.others == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\"></div>\n            {% endif %}\n            : \u0623\u062e\u0631\u0649\n        </div>\n\n        <!-- Sick Leave Column -->\n        <div style=\"flex: 1.5; text-align: center;\">\n            Sick Leave:\n            {% if doc.sick_leave == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\"></div>\n            {% endif %}\n            : \u0645\u0631\u0636\u064a\u0629\n        </div>\n\n        <!-- Emergency Column -->\n        <div style=\"flex: 1.5; text-align: right;\">\n            Emergency:\n            {% if doc.emergency == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\"></div>\n            {% endif %}\n            : \u0627\u0636\u0637\u0631\u0627\u0631\u064a\u0629\n        </div>\n\n        <!-- Annual Column -->\n        <div style=\"flex: 1; text-align: right;\">\n            Annual:\n            {% if doc.annual == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\"></div>\n            {% endif %}\n            : \u0633\u0646\u0648\u064a\u0629\n        </div>\n\n        <!-- Leave Type Column -->\n        <div style=\"flex: .75; text-align: right;font-weight:bold;\">\n            : \u0646\u0648\u0639 \u0627\u0644\u0625\u062c\u0627\u0632\u0629\n        </div>\n\n    </div>\n</div>\n\n\n\n<div style=\"display: flex; width: 100%; align-items: center; text-align: center;padding:2px;font-size:.8em;\">\n    <!-- Left Column (8%) -->\n    <div style=\"flex: 0 0 8%; text-align: left;\">\n        Reason:\n    </div>\n\n    <!-- Middle Column (84%) -->\n    <div style=\"flex: 1 1 84%; padding: 2px;\">\n        <p style=\"margin: 0; text-align: justify; direction: rtl;\"><u>{{ doc.leave_reason }}</u></p>\n    </div>\n\n    <!-- Right Column (8%) -->\n    <div style=\"flex: 0 0 8%; text-align: right;\">\n        :\u062d\u062f\u062f\n    </div>\n</div>\n\n\n\n\n<div style=\"display: flex; width: 100%;font-size:.8em;\">\n    <div style=\"flex: 1; padding: 2px; text-align: center;\">\n        Signature:\n           {% if doc.leave_signature %}\n                <img src=\"{{ doc.leave_signature }}\" alt=\"Signature\" style=\"width:150px; height:60px; margin-left: 5px;\">\n            {% else %}\n                <p style=\"display: inline; margin-left: 5px;\">No signature available</p>\n            {% endif %}  : \u0627\u0644\u062a\u0648\u0642\u064a\u0639  \n</div>\n</div>\n        \n    \n    \n    <div style=\"display: flex; width: 100%; align-items: center; text-align: center;padding:2px;font-size:.8em;border-top:1.5px solid black;\">\n    <!-- Left Column (8%) -->\n    <div style=\"flex: 0 0 25%; text-align: left;\">\n        Addresses during vacation:\n    </div>\n\n    <!-- Middle Column (84%) -->\n    <div style=\"flex: 1 1 50%; padding: 2px;\">\n        <p style=\"margin: 0; text-align: justify; direction: rtl;\"><u>{{ doc.address_during_vacation }}</u></p>\n    </div>\n\n    <!-- Right Column (8%) -->\n    <div style=\"flex: 0 0 25%; text-align: right;\">\n        : \u0639\u0646\u0648\u0627\u0646 \u0627\u0644\u0645\u0648\u0638\u0641 \u0623\u062b\u0646\u0627\u0621 \u0627\u0644\u0625\u062c\u0627\u0632\u0629\n    </div>\n    </div>\n\n\n\n\n<div style=\"display: flex; width: 100%;font-size:.8em;\">\n    <div style=\"flex: 1; padding: 2px; text-align: center;\">\n        Tel:\n           {{doc.tel}}  : \u0647\u0627\u062a\u0641  \n</div>\n</div>\n\n\n <div style=\"display: flex; width: 100%; align-items: center; text-align: center;padding:2px;font-size:.8em;border-top:1.5px solid black;\">\n    <!-- Left Column (8%) -->\n    <div style=\"flex: 0 0 50%;\">\n      <span style=\"float:left;\"> Name of the substitution in the vacation:</span> {{doc.file_no}} :\u0645\u0644\u0641 \u0631\u0642\u0645 \n    </div>\n\n   \n\n    <!-- Right Column (8%) -->\n    <div style=\"flex: 0 0 50%;\">\n      {{doc.name_of_the_substitution_in_the_vacations}}  <span style=\"float:right;\">: \u0639\u0646\u0648\u0627\u0646 \u0627\u0644\u0645\u0648\u0638\u0641 \u0623\u062b\u0646\u0627\u0621 \u0627\u0644\u0625\u062c\u0627\u0632\u0629   </span>\n    </div>\n</div>\n\n\n\n\n    </div>\n    <div style=\" width: 3%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; text-align: center;border-bottom: 1.5px solid black;padding: 2px;\">1</div>\n        <div style=\"height: auto; \n            writing-mode: vertical-rl; -moz-transform: rotate(-180deg); text-align: center; \n            d