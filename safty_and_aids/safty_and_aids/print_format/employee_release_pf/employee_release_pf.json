 "html": "{% if letter_head %}\n{{ letter_head }} \n{% endif %}\n\n<div style=\"margin-right:12px; margin-left:12px;\">\n\n<h2 style=\"text-align:center; border: 1.5px solid black; padding:2px; outline: 3px solid black; outline-offset: 2px; font-weight:bold;\">\n  {{ doc.select_print_heading or \"\u0625\u062e\u0644\u0627\u0621 \u0627\u0644\u0637\u0631\u0641 \u0644\u0644\u0645\u0648\u0638\u0641\" }}\n</h2>\n\n<div style=\"display: flex; width: 100%; border: 1.5px solid black; box-sizing: border-box;\">\n    <!-- Left box (40%) -->\n    <div style=\"width: 40%; border-right: none; box-sizing: border-box;\">\n        <div style=\"display: flex; flex-wrap: wrap; width: 100%; box-sizing: border-box;\">\n            <!-- Row 1, Column 1 -->\n            <div style=\"width: 50%; padding: 2px; border-bottom: 1.5px solid black; border-right: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                {{doc.name}}\n            </div>\n            <!-- Row 1, Column 2 -->\n            <div style=\"width: 50%; padding: 2px; border-bottom: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                \u0631\u0642\u0645 \u0627\u0644\u0646\u0645\u0648\u0630\u062c\n            </div>\n            <!-- Row 2, Column 1 -->\n            <div style=\"width: 50%; padding: 2px; border-right: 1.5px solid black; box-sizing: border-box; font-weight:bold;\">\n                {{ frappe.utils.formatdate(frappe.utils.nowdate()) }}\n            </div>\n            <!-- Row 2, Column 2 -->\n            <div style=\"width: 50%; padding: 2px; box-sizing: border-box; font-weight:bold;\">\n                \u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0625\u0635\u062f\u0627\u0631\n            </div>\n        </div>\n    </div>\n    \n    <!-- Right box (60%) -->\n    <div style=\"width: 60%; padding: 2px; border-left: 1.5px solid black; box-sizing: border-box; font-weight:bold; text-align:center;\">\n        \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0644\u0628\u0634\u0631\u064a\u0629 / \u0646\u0645\u0627\u0630\u062c \u0627\u0644\u0639\u0645\u0644<br>HUMAN RESOURCES MANUAL\n    </div>\n</div>\n\n<div style=\"margin-top:5px; width: 100%;  box-sizing: border-box;\">\n    \n    \n<div style=\"display: flex; width: 100%; border:1.5px solid black;\">\n    <div style=\"border-right: 1.5px solid black;  width: 3%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; text-align: center;border-bottom: 1.5px solid black;padding: 2px;\">1</div>\n        <div style=\"height: auto;  writing-mode: vertical-rl; \n            -webkit-transform: rotate(360deg); text-align: center; display: flex; \n            align-items: center; justify-content: center;padding: 2px;\">\n            Emp. Data\n        </div>\n    </div>\n    <div style=\"border-right: 1.5px solid black; width: 94%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; display: flex; align-items: center; justify-content: center;padding:2px;\">\n           \n           \n    <div style=\"display: flex; flex-direction: column; width: 100%; \">\n    <!-- First Row: 3 Columns -->\n    <div style=\"display: flex; width: 100%;font-size:0.8em;padding-top:2px;\">\n        <div style=\"flex: 1;  text-align: left;\">Nationality : {{doc.nationality}} : \u0627\u0644\u062c\u0646\u0633\u064a\u0629   </div>\n        <div style=\"flex: 1;  text-align: center;\">No : {{doc.employee_no}} : \u0631\u0642\u0645 \u0627\u0644\u0645\u0648\u0638\u0641   </div>\n        <div style=\"flex: 1.5;  text-align: right;\">Name : {{doc.employee_name}} : \u0627\u0644\u0627\u0633\u0645 </div>\n    </div>\n    <!-- Second Row: 2 Columns -->\n    <div style=\"display: flex; width: 100%;font-size:0.8em;padding-top:2px;\">\n        <div style=\"flex: 1;  text-align: left;\">Title : {{doc.title}} : \u0645\u0633\u0645\u0649 \u0627\u0644\u0648\u0638\u064a\u0641\u0629   </div>\n        <div style=\"flex: 1;  text-align: center;\">Section : {{doc.section}} : \u0627\u0644\u0642\u0633\u0645   </div>\n        <div style=\"flex: 1;  text-align: right;\">Location : {{doc.location}} : \u0627\u0644\u0625\u062f\u0627\u0631\u0629 </div>\n    </div>\n    \n     <div style=\"display: flex; width: 100%;font-size:0.8em;padding-top:2px;\">\n        <div style=\"flex: 1;  text-align: left;\">Clearance Reason   </div>\n        <div style=\"flex: 1;  text-align: center;\">\n            Exit &numsp;\n            {% if doc.exit == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\"></div>\n            {% endif %}\n            : \u0646\u0647\u0627\u064a\u0629 \u062e\u062f\u0645\u0629\n        </div>\n        <div style=\"flex: 1;  text-align: right;\">\n            Vacation &numsp;\n            {% if doc.vacation == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\"></div>\n            {% endif %}\n            : \u0625\u062c\u0627\u0632\u0629\n        </div>\n        <div style=\"flex: 1;  text-align: right;\">:\u0633\u0628\u0628 \u0625\u062e\u0644\u0627\u0621 \u0627\u0644\u0637\u0631\u0641</div>\n    </div>\n    \n\n\n\n</div>\n</div>\n\n\n\n\n\n    </div>\n    <div style=\" width: 3%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; text-align: center;border-bottom: 1.5px solid black;padding: 2px;\">1</div>\n        <div style=\"height: auto; \n            writing-mode: vertical-rl; -moz-transform: rotate(-180deg); text-align: center; \n            display: flex; align-items: center; justify-content: center;padding: 2px;\">\n            \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0645\u0648\u0638\u0641\n        </div>\n    </div>\n</div>\n<!---------------first------------>\n\n\n\n    <div style=\"margin-top:5px; width: 100%; box-sizing: border-box;border:1.5px solid black;\">\n    <div style=\"display: flex; width: 100%; border-collapse: collapse;\">\n    <div style=\"border-right: 1.5px solid black;  width: 3%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; text-align: center;border-bottom: 1.5px solid black;padding: 2px;\">2</div>\n        <div style=\"height: auto;  writing-mode: vertical-rl; \n            -moz-transform: rotate(360deg); text-align: center; display: flex; \n            align-items: center; justify-content: center;padding: 2px;\">\n            Related Managements\n        </div>\n    </div>\n    <div style=\"border-right: 1.5px solid black; width: 94%; display: flex; flex-direction: column;\">\n        <div style=\"height: auto; display: flex; align-items: center; justify-content: center;\">\n\n<div style=\"display: flex; flex-direction: column; width: 100%; box-sizing: border-box;\">\n    <!-- Row 1 -->\n    \n    \n\n    <div style=\"display: flex; width: 100%; align-items: center; text-align: center; font-size: .8em; padding: 2px;\">\n        <!-- Left Column (50%) -->\n        <div style=\"flex: 0 0 50%; text-align: left; font-weight: bold; font-size: 1em;\">\n            Employ Department\n        </div>\n    \n        <!-- Right Column (50%) -->\n        <div style=\"flex: 0 0 50%; text-align: right; font-weight: bold; font-size: 1em;\">\n            \u0625\u062f\u0627\u0631\u0629 \u0627\u0644\u0645\u0648\u0638\u0641\n        </div>\n    </div>\n\n    <!-- New Row for Nationality, No, and Name -->\n    <div style=\"display: flex; width: 100%; align-items: center; padding: 2px;font-size: .8em;\">\n        <div style=\"flex: 1; text-align: left;\">Date: {{doc.get_formatted('ed_date')}} : \u0627\u0644\u062a\u0627\u0631\u064a\u062e </div>\n        <div style=\"flex: 2; text-align: center;\">\n        Signature : \n        {% if doc.ed_signature %}\n        <img src=\"{{ doc.ed_signature }}\" alt=\"Signature\" style=\"width:120; height:50px;\">\n        {% else %}\n            <p style=\"display: inline; margin-left: 5px;\">No signature available</p>\n        {% endif %} \n        </div>\n        :  \u0627\u0644\u062a\u0648\u0642\u064a\u0639  \n        <div style=\"flex: 1.5; text-align: right;\">Name : {{doc.ed_name}} : \u0627\u0644\u0627\u0633\u0645</div>\n    </div>\n    \n     <div style=\"display: flex; width: 100%; align-items: center; padding: 2px;border-bottom: solid black 1.5px;font-size: .8em;\">\n        <div style=\"flex: 2.3; text-align: left;\">Reason : <p style=\"display:inline-block;\">{{doc.ed_reason}}</p> : \u062d\u062f\u062f </div>\n        <div style=\"flex: .9; text-align: center;\">\n        Not Clear &nbsp; \n        {% if doc.ed_not_clear == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\"></div>\n            {% endif %}\n            &nbsp;  \u0644\u0627 \u064a\u062e\u0644\u0649 \u0637\u0631\u0641\u0647   \n        </div>\n        \n        <div style=\"flex: .9; text-align: center;\">\n        Clear &nbsp; \n        {% if doc.ed_clear == 1 %}\n            <div style=\"display: inline-block; width: 12px; height: 12px; border: 1.5px solid black; margin-right: 4px;\">\n                <div style=\"font-size: 10px;\">&#10003;</div>\n            </div>\n            {% else %}\n            <div style=\"display: inline-block; width: 12px; heig